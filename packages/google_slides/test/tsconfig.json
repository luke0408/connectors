{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "outDir": "../bin",
    "paths": {
      "@wrtnlabs/connector-google-slides": ["../src/index.ts"],
      "@wrtnlabs/connector-google-slides/lib/*": ["../src/*"]
    },
    "plugins": [
      { "transform": "typescript-transform-paths" },
      { "transform": "typia/lib/transform" }
    ]
  },
  "include": ["../src", "./"]
}
